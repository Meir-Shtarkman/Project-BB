#ifndef _ADDRESS_H
#define _ADDRESS_H

#include <iostream>
using namespace std;


class Address
{
private:
	char* City;
	char* Street;
	int House_Number;
public:
	char* getCity()       const;
	char* getStreet()     const;
	int getHouse_Number() const;

	bool setCity(char* temp);
	bool setStreet(char* temp);
	bool setHouse_Number(const int& temp);
	
	void showAddress()    const;
	void freeAddress();
};



#endif // !_ADDRESS_H
